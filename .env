# Multiple Gemini API Keys (comma-separated)
# Add keys from different GCP accounts for maximum resilience
# Format: key1,key2,key3
GEMINI_API_KEYS=AIzaSyAAxx_account1,AIzaSyBBxx_account2,AIzaSyCCxx_account3,AIzaSyAAxx_account4,AIzaSyBBxx_account5,AIzaSyCCxx_account6.......


# Models to try (in priority order)
GEMINI_MODELS=gemini-2.5-flash,gemini-2.5-flash-lite,gemini-2.0-flash,gemini-2.0-flash-lite

# Retry Configuration
MAX_RETRIES=3
BASE_DELAY_SECONDS=1.0
MAX_DELAY_SECONDS=30.0

# Circuit Breaker Configuration
CIRCUIT_BREAKER_MAX_FAILURES=3
CIRCUIT_BREAKER_COOLDOWN_SECONDS=60

# Key Circuit Breaker (separate settings for API keys)
KEY_CIRCUIT_BREAKER_MAX_FAILURES=2
KEY_CIRCUIT_BREAKER_COOLDOWN_SECONDS=120
